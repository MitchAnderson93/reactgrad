{
  "title": "ReactGrad Starter",
  "description": "Build pixel-perfect react apps from JSON",
  "theme": "bootstrap",
  "pages": [
    {
      "path": "/",
      "components": [
        {
          "type": "Header",
          "props": {
            "text": "Welcome to ReactGrad"
          }
        },
        {
          "type": "Button",
          "props": {
            "to": "/about",
            "label": "Go to About Page"
          }
        },
        {
          "type": "Link",
          "props": {
            "to": "/about",
            "label": "Go to About Page"
          }
        }
      ]
    },
    {
      "path": "/about",
      "components": [
        {
          "type": "Header",
          "props": {
            "text": "About This Project"
          }
        },
        {
          "type": "Link",
          "props": {
            "to": "/",
            "label": "Back to Home"
          }
        }
      ]
    }
  ]
}